{
    "minimum-stability": "dev",
    "prefer-stable": true,
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "require": {
        "php": ">=8.1",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2.2",
        "doctrine/migrations": "^3.5.2",
        "doctrine/orm": "^2.13.4",
        "league/commonmark": "^2.4",
        "meshell/octopuspress-bundle": "@dev",
        "mobiledetect/mobiledetectlib": "^3.74",
        "sensio/framework-extra-bundle": "^6.2.9",
        "spatie/flysystem-dropbox": "^2.0.6",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/doctrine-bridge": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/filesystem": "6.3.*",
        "symfony/flex": "^2.2.3",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/http-client": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/messenger": "6.3.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/redis-messenger": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/security-csrf": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/validator": "6.3.*",
        "symfony/yaml": "6.3.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13.0",
        "phpstan/phpstan": "^1.3",
        "phpstan/phpstan-doctrine": "^1",
        "phpstan/phpstan-symfony": "^1.1",
        "phpunit/phpunit": "^9.5.23",
        "staabm/phpstan-dba": "^0.2.42",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/maker-bundle": "^1.45",
        "symfony/phpunit-bridge": "^5.4.11"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckfinder:download": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "test": "bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=-1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "6.3.*"
        },
        "symfony-bin-dir": "bin",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "public-dir": "web"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/TianLiangZhou/octopuspress-bundle"
        }
    ]
}
